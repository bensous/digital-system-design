LIBRARY ieee;
USE ieee.std_logic_1164.all;

ENTITY lpm_or2 IS
	PORT (
		data0x		: IN STD_LOGIC_VECTOR (25 DOWNTO 0);
		result		: OUT STD_LOGIC
	);
END lpm_or2;


ARCHITECTURE alpha OF lpm_or2 IS

BEGIN
	
	WITH data0x select
	result <=
		'0' when "00000000000000000000000000",
		'1' when others;

END alpha;